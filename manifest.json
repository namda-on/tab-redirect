{
    "manifest_version": 3,
    "name": "Hello world!",
    "version": "1.0",
    "description": "A hello world extension!",
    "permissions": ["storage"],
    "icons": {
      "128": "icons/icon128.png",
      "48": "icons/icon48.png",
      "16": "icons/icon16.png"
    },
    "action": {
      "default_icon": "icons/icon16.png",
      "default_popup": "popup/popup.html"
    },
    "chrome_url_overrides": {
      "newtab": "new-tab/tab.html"
    }
  }
  